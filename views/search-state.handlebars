<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Type</th>
            <th scope="col">Description</th>
            <th scope="col">Location</th>
            <th scope="col">City</th>
            <th scope="col">State</th>
        </tr>
    </thead>
    <tbody>
        {{#each report}}
        <tr>
            <th scope="row">1</th>
            <td>{{this.type}}</td>
            <td>{{this.description}}</td>
            <td>{{this.location}}</td>
            <td>{{this.city}}</td>
            <td>{{this.state}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>